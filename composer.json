{
    "name": "estaty/estaty",
    "description": "Real estate done right",
    "license": "MIT",
    "authors": [
        {
            "name": "Haralan Dobrev",
            "email": "hkdobrev@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "silex/silex": "~1.2",
        "twig/twig": "~1.15.1",
        "monolog/monolog": "~1.11",
        "doctrine/orm": "~2.4.6",
        "doctrine/migrations": "1.0.*@dev",
        "kurl/silex-doctrine-migrations-provider": "0.1.*",
        "gigablah/silex-oauth": "0.1.*",
        "symfony/form": "~2.3",
        "symfony/security": "~2.3",
        "lusitanian/oauth": "0.3.*",
        "ircmaxell/password-compat": "~1.0",
        "symfony/translation": "~2.6",
        "symfony/validator": "~2.6",
        "symfony/doctrine-bridge": "~2.6",
        "symfony/config": "~2.6",
        "symfony/monolog-bridge": "~2.6"
    },
    "require-dev": {
        "silex/web-profiler": "~1.0.3",
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "Estaty\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Estaty\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html=build/coverage",
        "test-ci": "phpunit --coverage-clover=build/coverage.clover"
    }
}
